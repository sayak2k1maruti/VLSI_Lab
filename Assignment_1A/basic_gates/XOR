:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x555b5295e7b0 .scope module, "XOR_test" "XOR_test" 2 1;
 .timescale 0 0;
v0x555b52985300_0 .var "A", 0 0;
v0x555b529853d0_0 .var "B", 0 0;
v0x555b529854a0_0 .net "Y", 0 0, L_0x555b529855a0;  1 drivers
S_0x555b52971f80 .scope module, "UUT" "XOR" 2 4, 3 1 0, S_0x555b5295e7b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "Y";
L_0x555b529855a0 .functor XOR 1, v0x555b52985300_0, v0x555b529853d0_0, C4<0>, C4<0>;
v0x555b5293c180_0 .net "A", 0 0, v0x555b52985300_0;  1 drivers
v0x555b5293bf20_0 .net "B", 0 0, v0x555b529853d0_0;  1 drivers
v0x555b529851e0_0 .net "Y", 0 0, L_0x555b529855a0;  alias, 1 drivers
    .scope S_0x555b5295e7b0;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "XOR_gate.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x555b5295e7b0 {0 0 0};
    %vpi_call 2 8 "$monitor", "%d XOR  %d -> %d", v0x555b52985300_0, v0x555b529853d0_0, v0x555b529854a0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555b52985300_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555b529853d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x555b529854a0_0;
    %cmpi/ne 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 10 "$display", "00 failed" {0 0 0};
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555b52985300_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555b529853d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x555b529854a0_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %vpi_call 2 12 "$display", "01 failed" {0 0 0};
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555b52985300_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555b529853d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x555b529854a0_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz  T_0.4, 4;
    %vpi_call 2 14 "$display", "10 failed" {0 0 0};
T_0.4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555b52985300_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555b529853d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x555b529854a0_0;
    %cmpi/ne 0, 0, 1;
    %jmp/0xz  T_0.6, 4;
    %vpi_call 2 16 "$display", "11 failed" {0 0 0};
T_0.6 ;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
