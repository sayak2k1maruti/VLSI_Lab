#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5596a38b47b0 .scope module, "AND_test" "AND_test" 2 1;
 .timescale 0 0;
v0x5596a38db900_0 .var "A", 0 0;
v0x5596a38db9d0_0 .var "B", 0 0;
v0x5596a38dbaa0_0 .net "Y", 0 0, L_0x5596a38dbba0;  1 drivers
S_0x5596a38c7f80 .scope module, "UUT" "AND" 2 4, 3 1 0, S_0x5596a38b47b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "Y";
L_0x5596a38dbba0 .functor AND 1, v0x5596a38db900_0, v0x5596a38db9d0_0, C4<1>, C4<1>;
v0x5596a3892180_0 .net "A", 0 0, v0x5596a38db900_0;  1 drivers
v0x5596a3891f20_0 .net "B", 0 0, v0x5596a38db9d0_0;  1 drivers
v0x5596a38db7e0_0 .net "Y", 0 0, L_0x5596a38dbba0;  alias, 1 drivers
    .scope S_0x5596a38b47b0;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "AND_gate.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5596a38b47b0 {0 0 0};
    %vpi_call 2 8 "$monitor", "%d AND  %d -> %d", v0x5596a38db900_0, v0x5596a38db9d0_0, v0x5596a38dbaa0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596a38db900_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596a38db9d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x5596a38dbaa0_0;
    %load/vec4 v0x5596a38db900_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %load/vec4 v0x5596a38db9d0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %vpi_call 2 10 "$display", "00 failed" {0 0 0};
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596a38db900_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596a38db9d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x5596a38dbaa0_0;
    %load/vec4 v0x5596a38db900_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %load/vec4 v0x5596a38db9d0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %vpi_call 2 12 "$display", "01 failed" {0 0 0};
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596a38db900_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596a38db9d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x5596a38dbaa0_0;
    %load/vec4 v0x5596a38db900_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %load/vec4 v0x5596a38db9d0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %vpi_call 2 14 "$display", "10 failed" {0 0 0};
T_0.4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596a38db900_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596a38db9d0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x5596a38dbaa0_0;
    %load/vec4 v0x5596a38db900_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %load/vec4 v0x5596a38db9d0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
